{
  "manifest_version": 3,
  "name": "TrackTern",
  "version": "2.1.0",
  "description": "Save job listings locally with smart extraction (not supported on LinkedIn).",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "TrackTern"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

